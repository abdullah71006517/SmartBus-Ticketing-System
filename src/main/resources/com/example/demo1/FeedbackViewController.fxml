            <TableColumn fx:id="usernameCol" prefWidth="120" text="Username" />
            <TableColumn fx:id="cleanlinessCol" prefWidth="120" text="Cleanliness" />
            <TableColumn fx:id="behaviorCol" prefWidth="150" text="Driver's Behavior" />
