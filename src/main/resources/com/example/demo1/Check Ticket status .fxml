      <Label fx:id="seat" layoutX="250.0" layoutY="302.0" text="---" />

      <Label layoutX="87.0" layoutY="333.0" text="📅 Date          :" textFill="#0e55fa">
         <font>
            <Font name="System Bold Italic" size="12.0" />
