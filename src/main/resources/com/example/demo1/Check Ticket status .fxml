      <Button fx:id="searchButton" layoutX="691.0" layoutY="71.0" onAction="#serachtikectno" style="-fx-background-color: green;" text="Search" textFill="WHITE">
         <font><Font name="System Bold" size="14.0" /></font>
      </Button>

      <Label layoutX="87.0" layoutY="212.0" text="🎫 Ticket No      :" textFill="#0e55fa">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font></Label>
      <Label fx:id="ticketno" layoutX="250.0" layoutY="212.0" text="---" />

      <Label layoutX="81.0" layoutY="244.0" text="👤 Passenger ID :" textFill="#0e55fa">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font></Label>
      <Label fx:id="passngername" layoutX="250.0" layoutY="244.0" text="---" />

      <Label layoutX="87.0" layoutY="274.0" text="🚌 Bus ID          :" textFill="#0e55fa">
