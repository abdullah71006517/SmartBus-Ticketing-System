      <Label fx:id="date" layoutX="250.0" layoutY="333.0" text="---" />

      <Label layoutX="87.0" layoutY="363.0" text="⏰ Time         :" textFill="#0e55fa">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font></Label>
      <Label fx:id="time" layoutX="250.0" layoutY="363.0" text="---" />

      <Label layoutX="87.0" layoutY="391.0" text="💰 Price         :" textFill="#0e55fa">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Label fx:id="price" layoutX="250.0" layoutY="391.0" text="---" />

      <BorderPane layoutX="55.0" layoutY="181.0" prefHeight="290.0" prefWidth="724.0" style="-fx-border-color: black;" />

      <Button fx:id="pdfButton" layoutX="302.0" layoutY="508.0" onAction="#dowloandaspdf" style="-fx-background-color: red;" text="Download as PDF" />
      <Button fx:id="backButton" layoutX="782.0" layoutY="23.0" onAction="#back" text="Back" />
      <Label layoutX="166.0" layoutY="112.0" prefHeight="18.0" prefWidth="623.0" text="  SmartBus - Digital Ticket" textFill="#6416ea" underline="true">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="554.0" layoutY="227.0" pickOnBounds="true" preserveRatio="true">
